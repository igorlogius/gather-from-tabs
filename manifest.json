{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/gather-from-tabs",
  "browser_action": {
    "default_area": "navbar"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "main.html"
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 2,
  "name": "Gather from Tabs",
  "permissions": ["<all_urls>", "clipboardWrite", "menus", "storage"],
  "version": "1.2.17"
}
