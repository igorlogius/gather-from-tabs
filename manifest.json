{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius",
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "options.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 2,
  "name": "Gather from Tabs",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["<all_urls>", "storage", "menus", "clipboardWrite", "tabs"],
  "optional_permissions": ["notifications"],
  "version": "1.0.5"
}
