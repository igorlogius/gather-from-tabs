{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/gather-from-tabs",
  "browser_action": {
    "default_area": "navbar"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "512": "icon.png"
  },
  "manifest_version": 2,
  "name": "Gather from Tabs",
  "permissions": ["storage", "tabs"],
  "optional_permissions": ["<all_urls>", "clipboardWrite"],
  "version": "1.1.9"
}
